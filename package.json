{
  "name": "puppeteer-utils",
  "version": "0.0.1",
  "description": "Puppeteer utils and config for WP",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/Automattic/puppeteer-utils.git"
  },
  "main": "lib/index.js",
  "scripts": {
    "build": "babel --no-babelrc src -d lib --presets=@babel/preset-env",
