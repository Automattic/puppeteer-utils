    "@wordpress/e2e-test-utils": "^3.0.0",
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "config": "^3.2.4",
    "eslint": "6.7.2",
    "jest": "^24.9.0",
    "prettier": "npm:wp-prettier@1.19.1",
    "puppeteer": "^2.0.0"
  }
}
